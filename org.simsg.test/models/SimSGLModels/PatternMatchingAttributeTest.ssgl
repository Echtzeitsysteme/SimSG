ModelID PatternMatchingAttributeTest

agent a[f:FLOAT, i:FLOAT]
agent b[x:FLOAT, y:FLOAT, z:FLOAT]

var p1 = {A:a()}[A.f=5.0, A.i=-2]
var p2 = {A:a()}[A.f=-4.0, A.i=3]
var p3 = {A:a()}[A.i<=3]
var p4 = {A:a()}[A.f>A.i]
var p5 = {A:a()}[A.f<A.i]
var p6 = {A:a()}[A.i>=A.f]

var p7 = {A:a()}[A.f+7=A.i]
var p8 = {A:a()}[sqrt(A.f+7)*-1<=A.i]

var p9 = {B:b()}[B.x=2, B.y=1.5, B.z=0.4]
var p10 = {A:a(), B:b()}[A.i>sqrt((B.x^2)+(B.y^2)+(B.z^2))]

init i1 3 p1
init i2 4 p2
init i3 2 p9

rule simpleAttributeCheck p1 -> {void} @ [1]
rule simpleAttributeCheck2 p2 -> {void} @ [1]
rule simpleAttributeCheck3 p3 -> {void} @ [1]
rule simpleAttributeCheck4 p4 -> {void} @ [1]
rule simpleAttributeCheck5 p5 -> {void} @ [1]
rule simpleAttributeCheck6 p6 -> {void} @ [1]

rule mediumAttributeCheck p7 -> {void} @ [1]
rule mediumAttributeCheck2 p8 -> {void} @ [1]

rule hardAttributeCheck p10 -> {void, void} @ [1]
